{
  "widget": "set-page",
  "content": [
    {
      "widget": "set-section",
      "options": { "title": "Kernel Log" },
      "content": [
        {
          "widget": "table",
          "if": "re=/^(?:\\[([ .0-9]*)\\])?\\s*(.*)$/i , log=ubus('luci2.system', 'dmesg').log",
          "options": {
            "dataSource=": [
              "log.split('\\n').map( (log, row) => (",
              "   m=re.exec(log) , m ? ",
              "     {log, row, time: parseFloat(m[1]), msg: m[2]} :",
              "     {log, row, msg: log }))"
            ],
            "columns": ["row", "time", "msg"],
            "headers": { "row": "Row", "time": "Time", "msg": "Message" },
            "colFormat": ["", "number:'1.6-6'"],
            "pageSizes": [10, 20, 50],
            "filter": true,
            "disableSort": ["msg"]
          }
        }
      ]
    }
  ]
}
