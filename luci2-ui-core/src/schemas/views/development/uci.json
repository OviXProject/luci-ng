{
  "widget": "form",
  "onInit": "configs = ubus('uci', 'configs').configs",
  "content": [
    {
      "widget": "autocomplete",
      "bind": "_.config",
      "options": {
        "enum=": "configs"
      }
    },
    {
      "widget": "uci-config",
      "displayIf": "configs.indexOf(_.config)>=0",
      "options": {
        "config=": "_.config"
      }
    }
  ]
}
