{
  "widget": "set-page",
  "if": "configs = ubus('uci', 'configs').configs",
  "content": [
    {
      "widget": "set-section",
      "bind": "$",
      "options": { "title": "UCI autogenerated" },
      "content": [
        {
          "widget": "set-select",
          "bind": "$.config",
          "options": { "title": "Configuration", "enum=": "configs" }
        }
      ]
    },

    {
      "widget": "set-section",
      "bind": "$",
      "options": { "title": "Generated View" },
      "if": "configs.indexOf($.config)>=0",
      "content": ["uciUI($.config)"]
    }
  ]
}
