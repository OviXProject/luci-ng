[
  {
    "widget": "set-level",
    "if": "board = ubus('system','board')",
    "options": {
      "value=": "board.model",
      "title=": "board.release.description",
      "icon": "information-variant",
      "noGraph": true
    },
    "content": [
      {
        "widget": "table",
        "options": {
          "columns": ["title", "value"],
          "dataSource": [
            {
              "title": "Hostname",
              "value=": "board.hostname"
            },
            {
              "title": "Kernel",
              "value=": "board.kernel"
            },
            {
              "title": "System",
              "value=": "board.system"
            },
            {
              "title": "Target",
              "value=": "board.release.target"
            }
          ]
        }
      }
    ]
  }
]
